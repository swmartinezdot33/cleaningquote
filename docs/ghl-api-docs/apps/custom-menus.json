            "example": false
          }
        }
      },
      "UpdateCustomMenuLinkResponseDTO": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean",
            "description": "Status of update"
          },
          "customMenu": {
            "description": "Updated custom menu link",
            "allOf": [
              {
                "$ref": "#/components/schemas/CustomMenuSchema"
              }
            ]
          }
        }
      }
